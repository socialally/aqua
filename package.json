{
  "name": "aqua-css",
  "description": "Stylus stylesheet library",
  "version": "0.3.1",
  "stability": "unstable",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "git://github.com/socialally/aqua.git"
  },
  "dependencies": {
    "kouto-swiss": "~0.11.13"
  },
  "devDependencies": {
    "del": "~1.2.0",
    "gulp": "~3.9.0",
    "gulp-size": "~1.2.3",
    "gulp-stylint": "~1.1.10",
    "gulp-stylus": "~2.0.6",
    "require-dir": "~0.3.0"
  },
  "scripts": {
    "clean": "gulp clean",
    "css": "gulp css",
    "docs": "npm run readme",
    "readme": "mdp --force -v"
  },
  "mdp": {
    "title": "Aqua",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/socialally/aqua",
    "partial": [
      {
        "ref": "stability",
        "format": "> `Stability: %s`"
      },
      {
        "inc": [
          "introduction.md",
          "install.md",
          "guide.md",
          "developer.md",
          "license.md"
        ]
      }
    ]
  }
}
